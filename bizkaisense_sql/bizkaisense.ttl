map:Stations_url a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Stations;
	d2rq:property rdfs:seeAlso;
	#d2rq:propertyDefinitionLabel "Stations url";
	d2rq:column "Stations.url";
	.

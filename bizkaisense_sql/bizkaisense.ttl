map:Stations_geonames a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Stations;
	d2rq:property dul:nearTo;
	#d2rq:propertyDefinitionLabel "Stations geonames";
	d2rq:column "Stations.geonames";
	.
